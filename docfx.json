{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "Fabric User Groups/images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "BI Documentation",
      "_appTitle": "BI Documentation",
      "_enableSearch": true,
      "pdf": false,
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/logo.png",
      "_appFooter": "Questions or requests? Submit a ticket via <a href='https://binaryconsultants.atlassian.net/servicedesk/customer/portal/27' target='_blank'>JIRA</a>.",
      "_editUrl": ""
    }
  }
}